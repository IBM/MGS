#ifndef LYPCOLLECTOR_MDL
#define LYPCOLLECTOR_MDL
#include "../PointNeuronTools/PointNeuronTools.mdl"

Interface LypFeed {
  double * x;
  }

Node LypCollector Implements OutputProducer 
{
  double * [] inputs;   
  double output;

  InitPhase initialize();
   
  RuntimePhase update(output);
  
  UserFunction setIndices;   

  OutputProducer.output << &output;

  InAttrPSet {
     string identifier;
   }

  Connection Pre Node (PSet.identifier == "MSN") Expects LypFeed {
   LypFeed.x >> inputs;
   setIndices();
   }



}

#endif



