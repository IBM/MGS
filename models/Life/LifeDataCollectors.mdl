#ifndef LIFENODEDATACOLLECTORS_MDL
#define LIFENODEDATACOLLECTORS_MDL

Variable LifeDataCollector
{
   string fileName;
   int [] rows;
   int [] cols;
   int mxcol;
   int* [] vals;
   UserFunction getNodeIndices;

   Connection Pre Node () Expects ValueProducer
   {
      ValueProducer.value >> vals;
      getNodeIndices();
   }
 
   InitPhase initialize;
   TriggeredFunction dataCollection;
   FinalPhase finalize;
}

#endif