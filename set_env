export MGSROOT=`pwd`
export NTSROOT=$MGSROOT
export NTSMGSROOT=$MGSROOT
export MDLROOT=$MGSROOT/mdl
export PATH=$MGSROOT/gsl/bin:$PATH
export GSLPARSER=$MGSROOT/gsl/bin/gslparser
if [ -f .built ]; then
sed -i "1s@.*@$MGSROOT@" .built
sed -i "2s@.*@@" .built
fi
